package com.nirvasoft.web.pos.model;
import java.util.ArrayList;

public class MenuOrderData {
	private String syskey;
	private String t2;
	private int n4;
	private boolean show;
	private ArrayList<MenuOrderData> child;

	public MenuOrderData() {
		clearProperties();
	}

	public String getSyskey() {
		return syskey;
	}

	public void setSyskey(String syskey) {
		this.syskey = syskey;
	}

	public String getT2() {
		return t2;
	}

	public void setT2(String t2) {
		this.t2 = t2;
	}

	public int getN4() {
		return n4;
	}

	public void setN4(int n4) {
		this.n4 = n4;
	}	

	public boolean isShow() {
		return show;
	}

	public void setShow(boolean show) {
		this.show = show;
	}

	public ArrayList<MenuOrderData> getChild() {
		return child;
	}

	public void setChild(ArrayList<MenuOrderData> child) {
		this.child = child;
	}

	protected void clearProperties() {
		this.syskey = "";
		this.t2 = "";
		this.n4 = 0;
		this.show = false;
		this.child = new ArrayList<MenuOrderData>();
	}
}
